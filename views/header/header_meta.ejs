</script>
<% } %>

<% if (ibmNPS) { %>
<script>
  window.ibmNps = {
    offeringId: '<%= ibmNPS.key %>',
    userId: '<%- userEmail %>'
  };
  <% if (environment != 'production') { %>
  window.ibmNps.test = true;
  <% } %>
  <% if (ibmNPS.attributes != {}) { %>
  window.ibmNps.customAttributes = <%- JSON.stringify(ibmNPS.attributes) %>
  <% } %>
</script>

<script id="ibmNpsSurveyScript" async src="//nps.mybluemix.net/survey/nps-survey.js"></script>
<% } %>

<% if (typeof(environment) !== 'undefined' && environment != '') { %>
<script>
  var environment =<%-JSON.stringify(environment)-%>;
</script>
<% } %>

