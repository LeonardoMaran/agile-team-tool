<c:choose>
  <c:when test="${fn:toLowerCase(pageTitle) == 'home'}">
    <c:set var="navHomeActive" value="${ACTIVE}" />
    <c:set var="navHomeSelected" value="${SELECTED}" />
  </c:when>
  <c:when test="${fn:toLowerCase(pageTitle) == 'team management'}">
    <c:set var="navTeamActive" value="${ACTIVE}" />
    <c:set var="navTeamSelected" value="${SELECTED}" />
  </c:when>
  <c:when test="${fn:toLowerCase(pageTitle) == 'iteration management'}">
    <c:set var="navIterActive" value="${ACTIVE}" />
    <c:set var="navIterSelected" value="${SELECTED}" />
  </c:when>
  <c:when test="${fn:toLowerCase(pageTitle) == 'maturity assessment' || fn:toLowerCase(pageTitle) == 'assessment summary'}">
    <c:set var="navAssessmentActive" value="${ACTIVE}" />
    <c:set var="navAssessmentSelected" value="${SELECTED}" />
  </c:when>
  <c:when test="${fn:toLowerCase(pageTitle) == 'help'}">
    <c:set var="navHelpActive" value="${ACTIVE}" />
    <c:set var="navHelpSelected" value="${SELECTED}" />
  </c:when>
</c:choose>

