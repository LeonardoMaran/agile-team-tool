<c:set var="ACTIVE" value="class=ibm-active" />
<c:set var="SELECTED" value="aria-selected=true" />
<c:set var="navHomeActive" value="" />
<c:set var="navHomeSelected" value="" />
<c:set var="navTeamActive" value="" />
